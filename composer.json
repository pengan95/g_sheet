{
    "name": "hopper/g_sheet",
    "description": "google sheet api tool",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hopperhuang",
            "email": "hopperhuang@meikaitech.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "7.1.*",
        "google/apiclient":"^2.9",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "MeiKaiGsuit\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": [
            "Drive",
            "Sheets"
        ]
    }
}
